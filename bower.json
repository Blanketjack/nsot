{
    "name": "nsot",
    "authors": [
        "Gary M. Josack <gary@dropbox.com>"
    ],
    "description": "NSoT is a Network Source of Truth API and Frontend for managing Network Assets.",
    "private": true,
    "ignore": [
        "**/.*",
        "node_modules",
        "bower_components",
        "vendor",
        "test",
        "tests"
    ],
    "dependencies": {
        "angular": "1.3.15",
        "angular-route": "1.3.15",
        "angular-resource": "1.3.15",
        "bootstrap": "3.3.2",
        "font-awesome": "4.3.0",
        "lodash": "3.7.0",
        "ng-tags-input": "2.3.0",
        "moment": "2.10.2",
        "jquery": "2.1.4"
    },
    "install": {
        "base": "nsot/static/vendor",
        "path": {
            "js": "{name}/js",
            "map": "{name}/js",
            "css": "{name}/css",
            "otf": "{name}/fonts",
            "eot": "{name}/fonts",
            "svg": "{name}/fonts",
            "ttf": "{name}/fonts",
            "woff": "{name}/fonts",
            "woff2": "{name}/fonts"
        },
        "sources": {
            "moment": "_bc/moment/min/moment.min.js",
            "ng-tags-input": [
                "_bc/ng-tags-input/ng-tags-input.min.*",
                "_bc/ng-tags-input/ng-tags-input.*.min.*"
            ],
            "lodash": "_bc/lodash/lodash.min.js",
            "font-awesome": [
                "_bc/font-awesome/css/font-awesome.min.css",
                "_bc/font-awesome/fonts/*"
            ],
            "angular-resource": [
                "_bc/angular-resource/angular-resource.min.js",
                "_bc/angular-resource/angular-resource.min.js.map"
            ],
            "angular-route": [
                "_bc/angular-route/angular-route.min.js",
                "_bc/angular-route/angular-route.min.js.map"
            ],
            "angular": [
                "_bc/angular/angular.min.js",
                "_bc/angular/angular.min.js.map"
            ],
            "bootstrap": [
                "_bc/bootstrap/dist/css/bootstrap.min.css",
                "_bc/bootstrap/dist/js/bootstrap.min.js"
            ],
            "jquery": [
                "_bc/jquery/dist/jquery.min.js",
                "_bc/jquery/dist/jquery.min.map"
            ]
        }
    }
}
